{
    "name": "pyper",
    "version": "0.0.9",
    "description": "An easy cli tool to create new python projects with a README.md, setup.py and manifest.json",
    "author": "Tanner Burns",
    "author_email": "tjburns102@gmail.com",
    "url": "https://github.com/TannerBurns/pyper",
    "requirements": [
        "click",
        "colored"
    ],
    "classifiers": [],
    "package_data": {
        "": [
            "build/templates/*",
            "build/templates/baseproject/*"
        ]
    },
    "package_include": [
        "pyper",
        "pyper.*"
    ],
    "package_exclude": [],
    "include_package_data": true,
    "entry_points": {
        "console_scripts": [
            "pyper=pyper:cli"
        ]
    },
    "scripts": [],
    "keywords": []
}